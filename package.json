{
  "name": "platform-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --host 0.0.0.0 --port 8080",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:ci": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 5",
    "preview": "vite preview",
    "reviewable": "npm run build && npm run lint && npm run cy:run",
    "cy:open": "cypress open --component --browser chrome --config-file ./cypress.config.ts",
    "cy:run": "cypress run --component --browser chrome --config-file ./cypress.config.ts",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write",
    "ci": "npm install"
  },
  "dependencies": {
    "@fontsource-variable/red-hat-text": "^5.0.19",
    "@fontsource/red-hat-display": "^5.0.20",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@tanstack/react-query": "^5.39.0",
    "@tanstack/react-router": "^1.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "usehooks-ts": "^3.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.39",
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@tanstack/router-devtools": "^1.34.0",
    "@tanstack/router-vite-plugin": "^1.33.12",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "happy-dom": "^14.10.1",
    "msw": "^2.3.0",
    "postcss": "^8.4.38",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "msw": {
    "workerDirectory": [
      "build",
      "public"
    ]
  }
}
